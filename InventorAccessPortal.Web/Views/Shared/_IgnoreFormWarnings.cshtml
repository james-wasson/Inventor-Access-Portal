@using InventorAccessPortal.Web.Util;
@using InventorAccessPortal.Web.Models;

@try
{
    if (Model != null)
    {
        bool? IsRightType = Model?.GetType()?.IsSubclassOf(typeof(_ToasterModel));
        if (IsRightType == true)
        {
            var castModel = (_ToasterModel)Model;
            castModel.IgnoreWarnings();
            if (castModel._IgnoredWarnings != null)
            {
                for (int i = 0; i < castModel._IgnoredWarnings.Count; i++)
                {
                    @Html.Hidden("_IgnoredWarnings[" + i + "]", castModel._IgnoredWarnings[i])
                }
            }
        }
    }
}
finally { }


