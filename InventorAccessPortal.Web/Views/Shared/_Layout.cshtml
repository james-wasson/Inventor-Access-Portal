@Html.Partial("_HeadersPartial")

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - Inventor Access Portal</title>
    @Styles.Render("~/Content/Lib/css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/Lib/modernizr")
</head>
<body>
    @Scripts.Render("~/bundles/Lib/jquery")
    @Scripts.Render("~/bundles/Lib/bootstrap")
    @Scripts.Render("~/bundles/scripts")
    <div class="wrapper">
        @Html.Partial("_SidebarPartial")
        <div id="content">
            @Html.Partial("_ErrorPartial")
            @RenderBody()
            @Html.Partial("_FooterPartial")
        </div>
    </div>


</body>
</html>
