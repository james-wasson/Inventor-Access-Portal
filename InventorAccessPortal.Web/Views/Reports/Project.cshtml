@model InventorAccessPortal.Web.Models.Reports.ProjectModel
@{
    ViewBag.Title = "Project Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <div>
        <h1>ProjectNum - ProjectTitle</h1>
    </div>
    <div class="project-info1">
        <ul>
            <li>Project Name: ...</li>
            <li>Lead Investigator: ...</li>
        </ul>
    </div>
    <div class="project-info2">
        <ul>
            <li>Status: ...</li>
            <li>Start Date: ...</li>
            <li>End Date: ...</li>
        </ul>
    </div>
    <div>
        <table class="table table-bordered table-responsive">
            <caption>Covered Files</caption>
            <thead>
                <tr>
                    <th>File Number</th>
                    <th>File Name</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>

            </tbody>
        </table>
    </div>
    <div>
        <table>
            <caption>Transactions</caption>
            <tr>
                <th>Date</th>
                <th>File Number</th>
                <th>Details</th>
                <th>Notes</th>
                <th>Activity</th>
            </tr>
        </table>
    </div>
</div>
